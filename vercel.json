{
    "version": 2,
    "builds": [
      {
        "src": "src/index.html",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist/angular-test"
        }
      },
      {
        "src": "api/**/*.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1.ts"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  