{
  "name": "quarantapiu-angular",
  "buildCommand": "ng build --configuration production",
  "outputDirectory": "dist/angular-test",
  "devCommand": "ng serve",
  "framework": "angular",
  "installCommand": "npm install",
  "headers": [
    {
      "source": "/(.*)\\.js$",
      "headers": [
        { "key": "Content-Type", "value": "application/javascript" }
      ]
    },
    {
      "source": "/(.*)\\.css$",
      "headers": [
        { "key": "Content-Type", "value": "text/css" }
      ]
    },
    {
      "source": "/(.*)\\.html$",
      "headers": [
        { "key": "Content-Type", "value": "text/html" }
      ]
    }
  ],
  "routes": [
    { 
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
